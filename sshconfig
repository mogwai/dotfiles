Host *
	StrictHostKeyChecking no
    ControlMaster auto
 	ControlPath /tmp/ssh-%r@%h:%pA
    ControlPersist yes
    ServerAliveInterval 10

Host fd-vpn
    Controlmaster auto
    User root
    Hostname labs.firedroplabs.com

Host labs
    HostName 10.8.0.6
    ProxyCommand ssh root@labs.firedroplabs.com nc %h %p
    User h
    ForwardAgent yes

